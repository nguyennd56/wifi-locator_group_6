<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wifilocator_rebuild"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_ow"
        android:label="@string/app_name"
        android:theme="@android:style/Theme" >
        <activity
            android:name="com.example.wifilocator_rebuild.StartUp"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity   android:name="com.example.wifilocator_rebuild.Tab" />
        <activity 	android:name="com.example.wifilocator_rebuild.SignalsActivity"/>
        <activity	android:name="com.example.wifilocator_rebuild.OnlineActivity"/>
        <activity	android:name="com.example.wifilocator_rebuild.LocationsActivity"/>
        <activity	android:name="com.example.wifilocator_rebuild.EditSignalPlaceActivity"/>
         <activity	android:name="com.example.wifilocator_rebuild.AddCurrentPlaceActivity"/>

    </application>

</manifest>